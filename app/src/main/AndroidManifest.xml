<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ensr.instagram"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />


    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name="com.ensr.instagram.AppController"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:largeHeap="true">
        <activity
            android:name="com.ensr.instagram.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.ensr.instagram.tabs.Export"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.ensr.instagram.tabs.Downloaded"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.ensr.instagram.tabs.Url"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.ensr.instagram.FullImage"
            android:label="@string/app_name" >
        </activity>

        <activity android:name="net.hockeyapp.android.UpdateActivity" />


    </application>

</manifest>
